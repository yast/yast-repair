{
    // testedfiles: OSRCommon.ycp

    include "testsuite.ycp";

    import "OSRCommon";


    define void TEST_IT (list<string> l1, list<string> l2) {

	DUMP (sformat ("is %1 subset of %2: %3",
	    l1, l2, OSRCommon::IsSubSet (l1, l2)));
    }

    TEST_IT ([], []);
    TEST_IT ([], ["a", "dfgdggh", "whatever"]);
    TEST_IT (["a", "b"], ["a"]);
    TEST_IT (["a"], ["b", "a"]);
    TEST_IT (["a", "b", "c", "d"], ["b", "a", "d", "d", "c"]);

}
