{
    include "testsuite.ycp";
    include "osr/osr_system.ycp";

    list l0 = [];
    list l1 = ["a", "b"];
    list l2 = ["a", "b", "c", "d"];

    TEST(``(OSRIsSubSet(l1, l2)), [], nil);  // true
    TEST(``(OSRIsSubSet(l2, l1)), [], nil);  // false
    TEST(``(OSRIsSubSet(l0, l1)), [], nil);  // true
    TEST(``(OSRIsSubSet(l0, l2)), [], nil);  // true
    TEST(``(OSRIsSubSet(l1, l0)), [], nil);  // false
    TEST(``(OSRIsSubSet(l2, l0)), [], nil);  // false
    TEST(``(OSRIsSubSet(l0, l0)), [], nil);  // true
    TEST(``(OSRIsSubSet(l1, l1)), [], nil);  // true
    TEST(``(OSRIsSubSet(l2, l2)), [], nil);  // true
}
