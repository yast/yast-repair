[Desktop Entry]
Type=Application
Categories=Settings;System;Qt;X-SuSE-YaST;X-SuSE-YaST-Misc;

X-KDE-ModuleType=Library
X-KDE-RootOnly=true
X-KDE-HasReadOnlyMode=true
X-KDE-Library=yast2
X-SuSE-YaST-Call=view_anymsg

X-SuSE-YaST-Group=Misc
X-SuSE-YaST-Argument=/var/log/boot.msg
X-SuSE-YaST-RootOnly=true
X-SuSE-YaST-AutoInst=
X-SuSE-YaST-Geometry=
X-SuSE-YaST-SortKey=
X-SuSE-YaST-AutoInstResource=

Icon=yast-bootmsg
Exec=/sbin/yast2 view_anymsg /var/log/boot.msg

Name=Start-Up Log
GenericName=Display the boot log (/var/log/boot.msg)
X-KDE-SubstituteUID=true
StartupNotify=true
